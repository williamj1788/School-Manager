{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "new-cap": ["error", { "properties": false }],
    "linebreak-style": "off"
  },
  "root": true,
  "ignorePatterns": ["/client"]
}
