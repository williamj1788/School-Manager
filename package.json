{
  "name": "school-manager",
  "version": "1.0.0",
  "engines": {
    "node": "10.15.0"
  },
  "description": "Allows student to manage thier classes better",
  "main": "index.js",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "node server.js",
    "client": "npm start --prefix client",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williamj1788/School-Manager.git"
  },
  "author": "Jacquez Williams",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/williamj1788/School-Manager/issues"
  },
  "homepage": "https://github.com/williamj1788/School-Manager#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "connect-mongo": "^2.0.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.19",
    "multer": "^1.4.1",
    "node-sass": "^4.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
